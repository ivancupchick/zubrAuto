{
  "name": "zubrAuto",
  "version": "1.0.0",
  "description": "node server",
  "main": "build/index.bundle.js",
  "scripts": {
    "start": "rimraf build && cp -r public build/ && cd ui && npm run build && cd .. && tsc && node ./build/index.js",
    "build": "rimraf build && mkdir build && cp -r public build/public/ && tsc && webpack && cd ui && npm run build",
    "dev": "rimraf build && cp -r public build/public/ && tsc && concurrently --kill-others \"tsc --watch\" \"nodemon ./build/index.js localhost 3080\" \"cd ui && ng build --watch\"",
    "dev-server": "rimraf build && cp -r public build/ && tsc && concurrently --kill-others \"tsc --watch\" \"nodemon ./build/index.js localhost 3080\"",
    "dev-ui": "cd ui && npm start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-server": "rimraf build && cp -r public build/ && tsc && node ./build/index.js"
  },
  "dependencies": {
    "aws-sdk": "^2.1050.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-validator": "^6.13.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "request-promise": "^4.2.6",
    "sharp": "^0.30.0",
    "ssh2": "^1.5.0",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.0",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mysql": "^2.15.19",
    "@types/node": "^16.10.1",
    "@types/request-promise": "^4.1.48",
    "@types/sharp": "^0.29.5",
    "@types/ssh2": "^0.5.51",
    "@types/uuid": "^8.3.1",
    "concurrently": "^6.3.0",
    "del": "^5.1.0",
    "fancy-log": "^1.3.3",
